@model IEnumerable<VeloOnline.Models.User>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Users list</h1>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>
                <a asp-action="Edit" asp-route-userId="@user.Id" class="btn btn-outline-primary">Change roles</a>
            </td>
        </tr>
    }
</table>


<a class="btn btn-warning" asp-action="Index">Roles</a>

